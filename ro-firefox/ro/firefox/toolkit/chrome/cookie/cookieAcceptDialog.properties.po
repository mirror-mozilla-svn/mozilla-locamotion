#: hostColon
msgid "Host:"
msgstr "Gazdă:"

#: domainColon
msgid "Domain:"
msgstr "Domeniu:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Numai conexiuni criptate"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Orice tip de conexiune"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "La sfârșitul sesiunii"

#: showDetails
msgid "Show Details"
msgstr "Arată detalii"

#: hideDetails
msgid "Hide Details"
msgstr "Ascunde detalii"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Site-ul %S dorește să stocheze un cookie."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Site-ul %S dorește să stocheze un al doilea cookie."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Site-ul %S dorește să stocheze un alt cookie.\n"
"Deja aveți %S cookie-uri de la acest site."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Site-ul %S dorește să modifice un cookie existent."
