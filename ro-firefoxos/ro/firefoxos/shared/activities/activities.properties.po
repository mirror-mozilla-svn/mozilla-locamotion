#. # LOCALIZATION NOTE These strings are used to describe available actions that
#. #   can be associated by default to an application.
#. #   e.g. "Open link" -> Firefox
#. #        "Open image"-> Gallery
#. #
#. # DIAL
#: default-activity-dialnumber
msgid "Make call"
msgstr "Efectuează apel"

#. # NEW
#. # default-activity-newcontact same as default-activity-opencontact
#: default-activity-newmail
msgid "Send email"
msgstr "Trimite e-mail"

#: default-activity-newsms
msgid "Send message"
msgstr "Trimite mesaj"

#. # OPEN
#: default-activity-opencontact
msgid "Open contact"
msgstr "Deschide contactul"

#: default-activity-openimage
msgid "Open image"
msgstr "Deschide imaginea"

#: default-activity-openaudio
msgid "Open audio"
msgstr "Deschide fișierul audio"

#: default-activity-openvideo
msgid "Open video"
msgstr "Deschide fișierul video"

#. # RECORD
#: default-activity-recordphoto
msgid "Take photo"
msgstr "Fă o fotografie"

#: default-activity-recordvideo
msgid "Record video"
msgstr "Înregistrează video"

#. # VIEW
#: default-activity-viewurl
msgid "Open link"
msgstr "Deschide linkul"

#. # default-activity-viewurl-email same as default-activity-newmail
#: default-activity-viewpdf
msgid "Open PDF file"
msgstr "Deschide fișierul PDF"
